<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0sp"
        android:left="0sp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <padding android:bottom="10sp"
                android:top="10sp" android:right="10sp"
                android:left="10sp"></padding>
            <corners android:radius="4sp"/>
            <stroke
                android:width="1dp"
                android:color="@color/black"
                android:dashWidth="5dp" />
        </shape>
    </item>
</layer-list>